create stream inputStream as Source(io.s4.latin.adapter.VfsFileReader,file=res:speech.in;type=JSON)
debugTest = select id,time,speaker from inputStream where "speaker" = 'franklin delano roosevelt' or "speaker" = 'richard m nixon'
persist stream debugTest to Output(io.s4.latin.persister.FilePersister,type=CSV;file=/tmp/filereaderoutput)
